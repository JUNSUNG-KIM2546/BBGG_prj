<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	
  	<!-- 언더바(스네이크형식)를 낙타표기(카멜) -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
  
  	<typeAliases>
		<typeAlias type="kr.ac.kopo.user.web.UserVO" alias="UserVO"/>
		<typeAlias type="kr.ac.kopo.part.web.PartVO" alias="PartVO"/>
  		<typeAlias type="kr.ac.kopo.user.web.ProUsersVO" alias="ProUsersVO"/>
  		<typeAlias type="kr.ac.kopo.note.web.NoteVO" alias="NoteVO"/>
  		<typeAlias type="kr.ac.kopo.room.web.RoomVO" alias="RoomVO"/>
  		<typeAlias type="kr.ac.kopo.report.web.ReportVO" alias="ReportVO"/>
  		<typeAlias type="kr.ac.kopo.list.web.ViewListVO" alias="ViewListVO"/>
  	</typeAliases>
  
  	<mappers>
  		 <mapper resource="mybatis/user.xml"/>
  		<mapper resource="mybatis/part.xml"/> 
  		<mapper resource="mybatis/pro.xml"/>
  		<mapper resource="mybatis/login.xml"/>
  		<mapper resource="mybatis/note.xml"/>
  		<mapper resource="mybatis/room.xml"/>
  		<mapper resource="mybatis/report.xml"/>
  		<mapper resource="mybatis/list.xml"/>
  	</mappers>
  </configuration>