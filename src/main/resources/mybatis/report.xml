<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="report">

	<select id="select">
		SELECT report.*, report_file.* 
		FROM report JOIN report_file ON (report.report_no = report_file.report_no)
	</select>
	
	<select id="selectList" resultMap=""></select>

	<insert id="">
		
	</insert>
	
	<update id=""></update>

</mapper>